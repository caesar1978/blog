<% title "Login" %>
<% form_for :session, :url => {:action => "create"} do |f| %>

<p><%= f.label :email, "Email address" %><br/>
<%= f.text_field :email %></p>

<p><%= f.label :password %><br/>
<%= f.text_field :password %></p>

<p><%= f.submit "Login"%></p>
<% end %>
<p>Find me in app/views/sessions/new.html.erb</p>
